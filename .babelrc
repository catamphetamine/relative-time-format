{
	"presets": [
		"@babel/preset-env"
	],

	"plugins": [
		["@babel/plugin-transform-for-of", { "loose": true }],
		"@babel/proposal-import-attributes-to-assertions",
		"@babel/plugin-syntax-import-assertions",
	],

	"env": {
		"es6": {
			"presets": [
				["@babel/preset-env", { "modules": false }]
			]
		},
		"test": {
			"plugins": ["istanbul"]
		}
	}
}